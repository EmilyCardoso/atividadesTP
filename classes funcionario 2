classe
   class Program
    {
        static void Main(string[] args)
        {
            Funcionario a = new Funcionario();
            Console.WriteLine("Digite o nome do funcionario ");
            a.Nome = Console.ReadLine();
            Console.WriteLine("digite o salario Bruto ");
            a.Salariobruto = double.Parse(Console.ReadLine());
            Console.WriteLine("digite o imposto dos saalrio ");
            a.imposto = double.Parse(Console.ReadLine());
            Console.WriteLine(a.Nome+ "tem o salario liquido de R$:"+a.SalarioLiquido());

            Console.ReadLine();
        }
    }
}


